<mvc:View controllerName="it.aupsup.inboundDelivery.controller.OrderPositionDetail" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.uxap" xmlns:layout="sap.ui.layout" xmlns:m="sap.m" xmlns:table="sap.ui.table" xmlns:f="sap.f" xmlns:core="sap.ui.core"
	xmlns:personal="sap.uxap.sample.SharedBlocks.personal" xmlns:form="sap.ui.layout.form">
	<m:Shell id="shell" appWidthLimited="false">
		<m:App id="app">
			<m:pages>
				<m:Page id="detail">
					<m:customHeader>
						<m:Bar>
							<m:contentLeft>
								<m:Text text="{OrderPrositionJSONModel>/EBELN}/{OrderPrositionJSONModel>/EBELP}"></m:Text>
							</m:contentLeft>
							<m:contentMiddle>
								<m:Text text="{i18n>appTitle}" class="sapMTitle sapMTitleStyleAuto sapMTitleMaxWidth"></m:Text>
							</m:contentMiddle>
						</m:Bar>
					</m:customHeader>
					<ObjectPageLayout id="ObjectPageLayout">
						<headerContent>
							<core:Fragment fragmentName="it.aupsup.inboundDelivery.fragments.DatiPosizione" type="XML"/>
						</headerContent>
						<sections>
							<ObjectPageSection title="{i18n>schedules}">
								<subSections>
									<ObjectPageSubSection title="">
										<m:Table id="ShedulersTable" items="{OrderPrositionJSONModel>/results/POItemConfirmations}">
											<m:columns>
												<m:Column id="SYSID" visible="false" minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>SYSID}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="EBELN" visible="false" minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>EBELN}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="EBELP" visible="false" minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>EBELP}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="ETENR" minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>ETENR}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="EINDT" minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>EINDT}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="LPEIN" minScreenWidth="Desktop" demandPopin="true" visible="false">
													<m:Label text="{i18n>LPEIN}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="MENGE" minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>MENGE_SCH}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="WEMNG" minScreenWidth="Desktop" demandPopin="true" visible="false">
													<m:Label text="{i18n>WEMNG}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column id="MNG02" minScreenWidth="Desktop" demandPopin="true" visible="false">
													<m:Label text="{i18n>MNG02}" class="columnLabelStyle"/>
												</m:Column>
											</m:columns>
											<m:items>
												<m:ColumnListItem>
													<m:cells>
														<m:Text text="{OrderPrositionJSONModel>SYSID}"/>
														<m:Text text="{OrderPrositionJSONModel>EBELN}"/>
														<m:Text text="{OrderPrositionJSONModel>EBELP}"/>
														<m:Text text="{OrderPrositionJSONModel>ETENR}"/>
														<m:Text text="{ path: 'OrderPrositionJSONModel>EINDT', type: 'it.aupsup.inboundDelivery.type.Date' }"/>
														<m:Text text="{OrderPrositionJSONModel>LPEIN}"/>
														<m:Text text="{= ${OrderPrositionJSONModel>MENGE} - ${OrderPrositionJSONModel>WEMNG}}"/>														
														<m:Text text="{path : 'OrderPrositionJSONModel>WEMNG', formatter : '.importFormatter'}"/>
														<m:Text text="{path : 'OrderPrositionJSONModel>MNG02', formatter : '.importFormatter'}"/>
														</m:cells>
												</m:ColumnListItem>
											</m:items>
										</m:Table>
									</ObjectPageSubSection>
								</subSections>
							</ObjectPageSection>
							<ObjectPageSection title="{i18n>confirmationsData}">
								<subSections>
									<ObjectPageSubSection title="">
										<m:Table id="ConfirmationsTableData" items="{OrderPrositionJSONModel>/results/POItemSchedulers/}">
											<m:columns>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>ETENS}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>EINDT}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true"  visible = "false">
													<m:Label text="{i18n>LPEIN}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true" visible = "false">
													<m:Label text="{i18n>XBLNR}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>MENGE}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true" visible = "false">
													<m:Label text="{i18n>DABMG}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true" visible = "false">
													<m:Label text="{i18n>MNG02}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true" visible="false">
													<m:Label text="{i18n>Status}" class="columnLabelStyle"/>
												</m:Column>
											</m:columns>
											<m:items>
												<m:ColumnListItem visible = "{= ${OrderPrositionJSONModel>TYPE} !== '2'}">
													<m:cells>
														<m:Text text="{OrderPrositionJSONModel>ETENS}"/>
														<m:Text text="{ path: 'OrderPrositionJSONModel>EINDT', type: 'it.aupsup.inboundDelivery.type.Date' }"/>
														<m:Text text="{OrderPrositionJSONModel>LPEIN}"/>
														<m:Text text="{OrderPrositionJSONModel>XBLNR}"/>														
														<m:Text text="{path : 'OrderPrositionJSONModel>MENGE', formatter : '.importFormatter'}"/>
														<m:Text text="{OrderPrositionJSONModel>DABMG}"/>
														<m:Text text="{OrderPrositionJSONModel>ESTKZ}"/>
														<m:Text text="{= ${OrderPrositionJSONModel>MENGE} - ${OrderPrositionJSONModel>WEMNG} > 0 ? '' : 'RICEVUTA'}"/>
													</m:cells>
												</m:ColumnListItem>
											</m:items>
										</m:Table>
									</ObjectPageSubSection>
								</subSections>
							</ObjectPageSection>

							<ObjectPageSection title="{i18n>confirmationsSpedizioni}">
								<subSections>
									<ObjectPageSubSection title="">
										<m:Table id="ConfirmationsTableSpedizioni" items="{OrderPrositionJSONModel>/results/POItemSchedulers/}">
											<m:headerToolbar>
												<m:OverflowToolbar>
													<m:content>
														<m:Title text="{i18n>confirmations}" level="H2"/>
													</m:content>
												</m:OverflowToolbar>
											</m:headerToolbar>
											<m:columns>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>ETENS}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>EINDT}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true"  visible = "false">
													<m:Label text="{i18n>LPEIN}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>XBLNR}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>MENGE}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true" visible = "false">
													<m:Label text="{i18n>DABMG}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true" visible = "false">
													<m:Label text="{i18n>MNG02}" class="columnLabelStyle"/>
												</m:Column>
												<m:Column minScreenWidth="Desktop" demandPopin="true">
													<m:Label text="{i18n>Status}" class="columnLabelStyle"/>
												</m:Column>
											</m:columns>
											<m:items>
												<m:ColumnListItem visible = "{= ${OrderPrositionJSONModel>TYPE} === '2'}">
													<m:cells>
														<m:Text text="{OrderPrositionJSONModel>ETENS}"/>
														<m:Text text="{ path: 'OrderPrositionJSONModel>EINDT', type: 'it.aupsup.inboundDelivery.type.Date' }"/>
														<m:Text text="{OrderPrositionJSONModel>LPEIN}"/>
														<m:Text text="{OrderPrositionJSONModel>XBLNR}"/>														
														<m:Text text="{path : 'OrderPrositionJSONModel>MENGE', formatter : '.importFormatter'}"/>															
														<m:Text text="{OrderPrositionJSONModel>DABMG}"/>
														<m:Text text="{OrderPrositionJSONModel>ESTKZ}"/>
														<m:Text text="{= ${OrderPrositionJSONModel>MENGE} - ${OrderPrositionJSONModel>DABMG} > 0 ? '' : 'RICEVUTA'}"/>
													</m:cells>
												</m:ColumnListItem>
											</m:items>
										</m:Table>
									</ObjectPageSubSection>
								</subSections>
							</ObjectPageSection>
						</sections>
					</ObjectPageLayout>
				</m:Page>
			</m:pages>
		</m:App>
	</m:Shell>
</mvc:View>