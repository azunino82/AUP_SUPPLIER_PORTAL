{
	"_version": "1.12.0",
	"sap.app": {
		"id": "it.aupsup.metasupplier_M",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle_M}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {
							"mode": {
								"required": true,
								"defaultValue": {
									"value": "M"
								}
							}
						},
						"additionalParameters": "allowed"
					},
					"semanticObject": "MetasupplierM",
					"action": "Display",
					"title": "{{appTitle_M}}",
					"icon": "sap-icon://customer-briefing"
				}
			}
		}
	},

	"sap.ui": {
		"fullWidth": true,		
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"config": {
			"fullWidth": true
		},		
		"flexEnabled": false,
		"rootView": {
			"viewName": "it.aupsup.metasupplier_M.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "it.aupsup.metasupplier_M.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "it.aupsup.metasupplier_M.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "TargetApp",
				"pattern": "app",
				"target": [
					"TargetApp"
				]
			}, {
				"name": "RouteMetasuppliers",
				"pattern": "home",
				"target": [
					"TargetMetasuppliers"
				]
			}, {
				"name": "RouteSuppliers",
				"pattern": "suppliers",
				"target": [
					"TargetSuppliers"
				]
			}, {
				"name": "RouteCreateMetasuppliers",
				"pattern": "create/metasuppliers/{suppliers}",
				"target": [
					"TargetCreateMetasuppliers"
				]
			}, {
				"name": "RouteMetasupplierContacts",
				"pattern": "metasuppliers/{metaid}/contacts",
				"target": [
					"TargetMetasupplierContacts"
				]
			}],
			"targets": {
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "App"
				},
				"TargetMetasuppliers": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Metasuppliers",
					"viewName": "Metasuppliers"
				},
				"TargetSuppliers": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Suppliers",
					"viewName": "Suppliers"
				},
				"TargetCreateMetasuppliers": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "CreateMetasuppliers",
					"viewName": "CreateMetasupplier"
				},
				"TargetMetasupplierContacts": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "MetasupplierContacts",
					"viewName": "MetasupplierContacts"
				}
			}
		}
	}
}