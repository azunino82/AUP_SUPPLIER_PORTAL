<mvc:View controllerName="it.alteaup.supplier.portal.metasupplier.AUPSUP_HTML5_METASUPPLIER.controller.View1" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					<Table items="{OutJSONModel>/results}">
					
											<columns>
												<Column id="EBELN" minScreenWidth="Desktop" demandPopin="true" width="6rem">
													<Label text="CLIENT ID" class="columnLabelStyle" wrapping="true"/>
												</Column>
												
											</columns>
											<items>
												<ColumnListItem>
													<cells>
														<Text text="{OutJSONModel>CUSTOMER}"/>
													</cells>
												</ColumnListItem>
											</items>
										</Table></content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>