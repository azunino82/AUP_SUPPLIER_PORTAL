	"LOG_MSG_NO" VARCHAR(6),
	"MESSAGE_V1" NVARCHAR(50),
	"MESSAGE_V2" NVARCHAR(50),
	"MESSAGE_V3" NVARCHAR(50),
	"MESSAGE_V4" NVARCHAR(50),
