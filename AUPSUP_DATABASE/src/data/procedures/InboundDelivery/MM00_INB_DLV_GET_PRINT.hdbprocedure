PROCEDURE "AUPSUP_DATABASE.data.procedures.InboundDelivery::MM00_INB_DLV_GET_PRINT"(
IN userid VARCHAR(250),
IN I_OBJKY NVARCHAR(30),
OUT stream BLOB) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	--DEFAULT SCHEMA <default_schema_name>
	READS SQL DATA AS
BEGIN

DECLARE I_KAPPL NVARCHAR(2);
DECLARE I_KSCHL NVARCHAR(4);
DECLARE counter INTEGER; 
DECLARE E_DATA BLOB;
IF (I_OBJKY <> '') THEN 
	SELECT 'V6' into I_KAPPL FROM "AUPSUP_DATABASE.data.synonyms::DUMMY"; /*EF ordine di vendita*/
	SELECT 'A001' into I_KSCHL FROM "AUPSUP_DATABASE.data.synonyms::DUMMY";
	CALL "AUPSUP_DATABASE.data.virtualProcedures::VIRTUAL_MM00_INB_DLV_GET_PRINT"(:I_KAPPL,:I_KSCHL,:I_OBJKY,:E_DATA);

SELECT :E_DATA INTO stream FROM "AUPSUP_DATABASE.data.synonyms::DUMMY";

END IF; 

END;
