PROCEDURE "AUPSUP_DATABASE.data.procedures.Metasupplier::DeleteMetasupplier"( IN METAID NVARCHAR (50)
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	--DEFAULT SCHEMA <default_schema_name>
	AS
BEGIN

DELETE FROM "AUPSUP_DATABASE.data.tables::T_METASUPPLIER_DATA" WHERE METAID = :METAID;
DELETE FROM "AUPSUP_DATABASE.data.tables::T_METAID_FORN" WHERE METAID = :METAID;
DELETE FROM "AUPSUP_DATABASE.data.tables::T_METAID_BU" WHERE METAID = :METAID;

END;
