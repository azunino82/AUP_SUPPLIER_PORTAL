PROCEDURE "AUPSUP_DATABASE.data.procedures.Utils::GetLocks"(
	IN userid NVARCHAR(250),
	IN IN_ITEMS TABLE (EBELN NVARCHAR(10), EBELP VARCHAR(5)),
	OUT out_table TABLE(EBELN NVARCHAR(10), EBELP VARCHAR(5))
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	READS SQL DATA AS
BEGIN

	out_table = SELECT EBELN,EBELP FROM "AUPSUP_DATABASE.data.tables::T_LOCKS" WHERE (EBELN,EBELP) IN (SELECT EBELN,EBELP FROM :IN_ITEMS);

END;