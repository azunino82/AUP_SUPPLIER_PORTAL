PROCEDURE "AUPSUP_DATABASE.data.procedures.Utils::UnlockItems"(
	IN userid NVARCHAR(250),
	IN IN_ITEMS TABLE (EBELN NVARCHAR(10), EBELP VARCHAR(5))
	) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
AS
	
BEGIN
	DELETE FROM "AUPSUP_DATABASE.data.tables::T_LOCKS" WHERE (EBELN,EBELP) IN (SELECT EBELN,EBELP FROM :IN_ITEMS);
END;