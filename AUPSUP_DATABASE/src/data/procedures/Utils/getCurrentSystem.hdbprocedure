PROCEDURE "AUPSUP_DATABASE.data.procedures.Utils::getCurrentSystem"(
	OUT OUT_TABLE TABLE (CUSTOMER NVARCHAR(30))
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
   OUT_TABLE = SELECT CUSTOMER FROM "AUPSUP_DATABASE.data.tables::T_BCKND_SYSTEMS";
END